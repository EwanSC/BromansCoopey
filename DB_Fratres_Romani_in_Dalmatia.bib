@incollection{Sanader2013,
address = {Zagreb},
annote = {Summ:
The authors compare the four stelae and their poems with one another, and with other known poetic stelae (mostly from Italy and Dalmatia), making a number of interesting findings. Firstly, there are similarities in phrase and pattern which suggests some kind of ‘poets manual' in the Empire, with pre-made, or partially made poems/poem structures. Two Legio VII poems (Q. Baebius and G. Iulius) are very similar in style, with similar phrases and seem to be from same person – same poet at Tilurium? Stelae of Ancharenus is v different but may be still written by same poet. The final stelae (L. Mummius), with the unique poem, was almost definitely not same poet, with more complex style (and visuals too). All four are from Asia (the soldiers). The stelae 1, 2, 3 have very similar iconography too. In fact, they fit into the broader style of Legio VII, just with the addition of a poem. 4th stela of Mummius, probably from another wokshop – inscriptions are better carved (*perhaps from a more skilled craftsman in Salona?*). No cognomen (or cpf), meaning pre-Claudius.},
author = {Sanader, Mirjana and Demicheli, Dino and {Mili{\'{c}}evi{\'{c}} Brada{\v{c}}}, Marina},
booktitle = {XVII Roman Military Equipment Conference, Zagreb, 24 - 27 May, 2010. Proceedings of the XVIIth Roman Military Equipment Conference. Weapons and Military Equipment in a Funerary Context},
